        <script>
            // Prepare animation
            var xStart = window.innerWidth/4;
            window.firstLoadIndexAnimation(reverseFlag=true);
            $(document).ready(() => {
                // Initialize grouped classes CSS
                $('.GC0').show();
                $('.GC1').hide();
                $('.GC2').hide();
                $('.GC3').hide();

                // Load color scheme
                var colors;
                console.log();
                $.getJSON('/public/assets/json/colorscheme.json', colorsJSON => {
                    colors = window.loadColorTheme(colorsJSON, "Light");
                    window.initializeCSSColorTheme(colors);
                    // LOADING SCREEN  
                    // Get the total number of resources on the page
                    var totalResources = $('*');
                    // Set the initial number of resources loaded to 0
                    var resourcesLoaded = 0;
                    // Set counters for animation
                    var percentageLoaded;
                    // For each resource that has to be loaded...
                    $('*').each(function () {
                        // ...check if resource exists in the DOM, i.e. has been loaded.
                        // (If not, length = 0)
                        if($(this).length) {                            
                            resourcesLoaded++;
                            percentageLoaded = resourcesLoaded/totalResources.length*100;
                            $('.loadingscreenText').text(`${percentageLoaded}%`);
                            if (resourcesLoaded === totalResources.length) {
                                // Fade out loading screen and delete for index page to show
                                $('.loadingscreenLogo').stop(true);
                                $('.loadingscreenLogo').fadeOut(1000, ()=>{});
                                $('.loadingscreenText').fadeOut(1000, ()=>{});
                                window.firstLoadIndexAnimation(reverseFlag=false);

                                /* ----------------------------------------------------------------------
                                ---------------------- INDEX PAGE CODE STARTS HERE ----------------------
                                ---------------------------------------------------------------------- */

                                // MENU ANIMATIONS
                                $('#h2menu0').mouseenter(() => {
                                    window.hoverMenupoint(0, 500, colors, reverseFlag=false);
                                });
                                $('#h2menu0').mouseleave(() => {
                                    window.hoverMenupoint(0, 500, colors, reverseFlag=true);
                                });
                                $('#h2menu1').mouseenter(() => {
                                    window.hoverMenupoint(1, 500, colors, reverseFlag=false);
                                });
                                $('#h2menu1').mouseleave(() => {
                                    window.hoverMenupoint(1, 500, colors, reverseFlag=true);
                                });
                                $('#h2menu2').mouseenter(() => {
                                    window.hoverMenupoint(2, 500, colors, reverseFlag=false);
                                });
                                $('#h2menu2').mouseleave(() => {
                                    window.hoverMenupoint(2, 500, colors, reverseFlag=true);
                                });
                                $('#h2menu3').mouseenter(() => {
                                    window.hoverMenupoint(3, 500, colors, reverseFlag=false);
                                });
                                $('#h2menu3').mouseleave(() => {
                                    window.hoverMenupoint(3, 500, colors, reverseFlag=true);
                                });

                                // EXPLORE ANIMATIONS
                                const initialFontWeight = parseInt($(".exploreText").css("font-weight"));
                                const initialStrokeWidth = parseInt($(".exploreArrowPolygon").css("stroke-width"));
                                $('#explore').mouseenter(() => {
